---
import { ArrowRight, List } from "@phosphor-icons/react";
---

<div class="flex items-center justify-between">
  <a href="/" aria-label="Home">
    <p class="text-3xl font-bold">
        <img src="/logo.png" alt="Bagas Miftahun" class="w-24">
    </p>
  </a>
  <div class="flex items-center gap-x-4">
    <a href="/contact">
      <button
        class="hidden md:block px-5 py-3 rounded-full border border-neutral-100 bg-neutral-0 text-neutral-100 hover:bg-neutral-100 hover:text-neutral-0"
      >
        <div class="flex items-center gap-x-3">
          <p class="font-semibold text-base">Let's Talk</p>
          <ArrowRight size={20} />
        </div>
      </button>
    </a>

    <button
      class="px-6 py-3 rounded-full border border-neutral-100 bg-neutral-0 text-neutral-100 hover:bg-neutral-100 hover:text-neutral-0"
    >
        <List size={24} />
    </button>
  </div>
</div>
